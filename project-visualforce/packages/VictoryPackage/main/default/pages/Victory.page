<apex:page controller="VictoryController" lightningStylesheets="True">
 
    <body> 
        <div class="slds-scope">
            <article class="slds-card">
                <div class="slds-card__header slds-grid">
                  <header class="slds-media slds-media_center slds-has-flexi-truncate">
                    <div class="slds-media__body">
                      <h2 class="slds-card__header-title">Winning team is : 
                        <a href="#" class="slds-card__header-link slds-truncate " title="Winning Team"> {!WinningTeam} </a>
                      </h2>
                    </div>
                    <div class="slds-card__body slds-card__body_inner">{!WinningPoke}</div>
                    <apex:image url="{!WinningPokeImage}" />  
                  </header>
                </div>
              </article>
        </div>
    </body>
</apex:page>